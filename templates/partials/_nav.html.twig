
<nav class="navbar bg-secondary">
  <div class="container-fluid">
    <a class="nav-link" aria-current="page" href={{ path('home')}}>
      <img src={{asset("images/logo.png")}} alt="logo" width="50" height="50">
    </a>
    {% if app.user %}
    <div>
      <a href={{path("compte")}} class="btn btn-info">Compte</a>
      <a href={{path("app_logout")}} class="btn btn-outline-info">Deconnexion</a>
    </div>
    {% else %}
        {% if app.request.attributes.get('_route') == 'app_login' %}
          <a href={{path("app_register")}} class="btn btn-info">S'inscrire</a>
        {% else %}
          <a href={{path("app_login")}} class="btn btn-info">Se connecter</a>
        {% endif %}
    {% endif %}
  </div>
</nav>
